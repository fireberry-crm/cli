{
  "name": "@fireberry/cli",
  "version": "0.3.0",
  "description": "Fireberry CLI tool",
  "type": "module",
  "author": "",
  "license": "MIT",
  "bin": {
    "fireberry": "dist/bin/fireberry.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node dist/bin/fireberry.js",
    "build": "tsc",
    "build:dev": "npm run clean && tsc && npm link && chmod +x dist/bin/fireberry.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "node test/smoke.test.js",
    "version:beta": "npm version prerelease --preid=beta",
    "publish:beta": "npm run version:beta && npm publish --tag beta",
    "publish:prod": "npm publish --tag latest"
  },
  "keywords": [
    "cli",
    "fireberry",
    "tool"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireberry-crm/cli.git"
  },
  "bugs": {
    "url": "https://github.com/fireberry-crm/cli/issues"
  },
  "homepage": "https://github.com/fireberry-crm/cli#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "env-paths": "^3.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "ora": "^8.0.1",
    "tar": "^7.5.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.24",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.3.3"
  }
}
